{
  "expo": {
    "name": "StarChat",
    "slug": "StarChat",
    "version": "1.0.0",
    "sdkVersion": "44.0.0",
    "orientation": "portrait",
    "icon": "./assets/newIcon.png",
    "splash": {
      "image": "./assets/chat.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mscc.o2ng",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/newAdaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/newFavicon.png"
    }
  }
}
